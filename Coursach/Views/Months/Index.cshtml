@model IEnumerable<Coursach.Month>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Month_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Work_Days_Amount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Month_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Work_Days_Amount)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Month_Code }) |
            @Html.ActionLink("Details", "Details", new { id=item.Month_Code }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Month_Code })
        </td>
    </tr>
}

</table>
